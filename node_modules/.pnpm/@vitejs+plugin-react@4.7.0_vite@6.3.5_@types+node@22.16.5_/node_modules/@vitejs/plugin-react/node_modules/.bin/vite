#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/zhaoyadong/Zhifou/react-router-v7-jsx/node_modules/.pnpm/vite@6.3.5_@types+node@22.16.5/node_modules/vite/bin/node_modules:/Users/zhaoyadong/Zhifou/react-router-v7-jsx/node_modules/.pnpm/vite@6.3.5_@types+node@22.16.5/node_modules/vite/node_modules:/Users/zhaoyadong/Zhifou/react-router-v7-jsx/node_modules/.pnpm/vite@6.3.5_@types+node@22.16.5/node_modules:/Users/zhaoyadong/Zhifou/react-router-v7-jsx/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/zhaoyadong/Zhifou/react-router-v7-jsx/node_modules/.pnpm/vite@6.3.5_@types+node@22.16.5/node_modules/vite/bin/node_modules:/Users/zhaoyadong/Zhifou/react-router-v7-jsx/node_modules/.pnpm/vite@6.3.5_@types+node@22.16.5/node_modules/vite/node_modules:/Users/zhaoyadong/Zhifou/react-router-v7-jsx/node_modules/.pnpm/vite@6.3.5_@types+node@22.16.5/node_modules:/Users/zhaoyadong/Zhifou/react-router-v7-jsx/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vite@6.3.5_@types+node@22.16.5/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../../vite@6.3.5_@types+node@22.16.5/node_modules/vite/bin/vite.js" "$@"
fi
